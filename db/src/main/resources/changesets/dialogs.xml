<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet id="createTable dialogs" author="Daniil Epikhov">
        <createTable tableName="dialogs">
            <column autoIncrement="true" name="id" type="bigint">
                <constraints nullable="false" primaryKey="true" primaryKeyName="dialogs_pkey"/>
            </column>
            <column name="companion_1" type="bigint"/>
            <column name="companion_2" type="bigint"/>
        </createTable>
    </changeSet>
</databaseChangeLog>

